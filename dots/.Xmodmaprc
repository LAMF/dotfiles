! make layout lisp machine like
! see http://www.rgrjr.com/emacs/i586.xmodmaprc
! control is control => change to super/mod4
! mod1 is Meta/Alt => change to control
! mod2 is Num_Lock 
! mod3 is nothing
! mod4 is Super => change to mod1/Meta/alt
! mod5 is ISO_Level3_Shift, Mode_switch

clear control
clear mod1
clear mod2
clear mod3
clear mod4
clear mod5

keycode 64 = Control_L
keycode 37 = Super_L
keycode 133 = Alt_L
keycode 105 = Super_R
keycode 135 = Alt_R
keycode 108 = Control_R

add control = Control_L Control_R
add mod1 = Alt_L Alt_R
add mod2 = Num_Lock
add mod4 = Super_L Super_R
add mod5 = ISO_Level3_Shift Mode_switch
